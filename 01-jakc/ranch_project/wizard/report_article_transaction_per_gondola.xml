<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <!--  Article Per Gondola -->
        <record id="wizard_article_transaction_per_gondola" model="ir.ui.view">
            <field name="name">Article Per Gondola</field>
            <field name="model">wizard.article.transaction.per.gondola</field>
            <field name="arch" type="xml">
                <form string="Article Per Gondola">
                    <group col="4" string="Dates">
                        <field name="stock_inventory_periode_id"/>
                        <field name="gondola_id"/>
                    </group>
                    <footer>
                        <button name="print_report" type="object" string="Print Report" class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>


        <record id="action_report_pos_details" model="ir.actions.act_window">
            <field name="name">Sale Details</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">pos.details</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>


    </data>
</openerp>